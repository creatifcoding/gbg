{
  "name": "ore",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ore/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build",
        "cwd": "packages/ore"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dev",
        "cwd": "packages/ore"
      }
    },
    "tauri:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run tauri:dev",
        "cwd": "packages/ore"
      }
    },
    "tauri:build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run tauri:build",
        "cwd": "packages/ore"
      }
    }
  },
  "tags": ["type:app", "scope:tools"]
}
