{
  "name": "obsidian-reverse-engineer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/obsidian-reverse-engineer/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/obsidian-reverse-engineer"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "packages/obsidian-reverse-engineer"
      }
    },
    "tauri:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run tauri:dev",
        "cwd": "packages/obsidian-reverse-engineer"
      }
    },
    "tauri:build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run tauri:build",
        "cwd": "packages/obsidian-reverse-engineer"
      }
    }
  },
  "tags": ["type:app", "scope:tools"]
}
